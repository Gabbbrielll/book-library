<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/white"
    tools:context=".Profile">

    <ImageView
        android:id="@+id/imageView19"
        android:layout_width="369dp"
        android:layout_height="531dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/booke_removebg_preview" />

    <ImageView
        android:id="@+id/black6"
        android:layout_width="332dp"
        android:layout_height="423dp"
        android:alpha="0.5"
        android:background="@color/material_dynamic_secondary10"
        android:backgroundTint="@color/material_dynamic_secondary10"
        android:src="@color/material_dynamic_secondary10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.464"
        tools:srcCompat="@color/material_dynamic_neutral_variant10" />
    <ImageView
        android:id="@+id/black5"
        android:layout_width="413dp"
        android:layout_height="800dp"
        android:alpha="0.5"
        android:background="@color/material_dynamic_secondary10"
        android:backgroundTint="@color/material_dynamic_secondary10"
        android:src="@color/material_dynamic_secondary10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:srcCompat="@color/material_dynamic_neutral_variant10" />




    <ImageView
        android:id="@+id/imageView18"
        android:layout_width="334dp"
        android:layout_height="261dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/bookw_removebg_preview" />

    <ImageView
        android:id="@+id/black3"
        android:layout_width="55dp"
        android:layout_height="43dp"
        android:alpha="0.5"
        android:background="@color/material_dynamic_secondary10"
        android:backgroundTint="@color/material_dynamic_secondary10"
        android:src="@color/material_dynamic_secondary10"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        tools:srcCompat="@color/material_dynamic_neutral_variant100" />


    <androidx.cardview.widget.CardView
        android:layout_width="203dp"
        android:layout_height="182dp"
        app:cardBackgroundColor="@color/material_dynamic_neutral100"
        app:cardCornerRadius="75dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.292">

        <ImageView
            android:id="@+id/black4"
            android:layout_width="211dp"
            android:layout_height="190dp"
            android:alpha="0.5"
            android:background="@color/material_dynamic_secondary10"
            android:backgroundTint="@color/material_dynamic_secondary10"
            android:src="@color/material_dynamic_secondary10"
            tools:srcCompat="@color/cardview_dark_background" />

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="230dp"
            android:layout_height="212dp"

            app:srcCompat="@drawable/be98caa3027999ec95e552c5eeef62da_removebg_preview" />
    </androidx.cardview.widget.CardView>
    <EditText
        android:id="@+id/user"
        android:layout_width="228dp"
        android:layout_height="52dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/saira_stencil_one"
        android:hint="Username"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textColorHint="@color/material_dynamic_neutral10"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.553" />

    <EditText
        android:id="@+id/fn"
        android:layout_width="228dp"
        android:layout_height="52dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/saira_stencil_one"
        android:hint="Name"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textColorHint="@color/material_dynamic_neutral10"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.63" />

    <EditText
        android:id="@+id/email"
        android:layout_width="228dp"
        android:layout_height="52dp"
        android:background="@android:color/transparent"
        android:ems="10"
        android:fontFamily="@font/saira_stencil_one"
        android:hint="Email"
        android:inputType="text"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textColorHint="@color/material_dynamic_neutral10"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.706" />

    <Button
        android:id="@+id/backbtn"
        android:layout_width="52dp"
        android:layout_height="44dp"
        android:background="@drawable/x_removebg_preview"
        android:onClick="toreading"
        app:backgroundTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />


    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="334dp"
        android:layout_height="261dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        app:srcCompat="@drawable/bookw_removebg_preview" />

    <Button
        android:id="@+id/logout"
        android:layout_width="297dp"
        android:layout_height="67dp"
        android:backgroundTint="@color/black"
        android:fontFamily="@font/saira_stencil_one"
        android:onClick="toMainActivity"
        android:text="Logout"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.916" />




</androidx.constraintlayout.widget.ConstraintLayout>